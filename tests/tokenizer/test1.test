["<!DOCTYPE html>", [["DOCTYPE", "HTML", False]], "Correct Doctype lowercase"]
["<!DOCTYPE HTML>", [["DOCTYPE", "HTML", False]], "Correct Doctype uppercase"]
["<!DOCTYPE HtMl>", [["DOCTYPE", "HTML", False]], "Correct Doctype mixed case"]
["<h>", [["StartTag", "h", {}]], "Single Start Tag"]
["<h a='b'>", [["StartTag", "h", {'a':'b'}]], "Start Tag w/attribute"]
["<h></h>", [["StartTag", "h", {}], ["EndTag", "h"]], "Start/End Tag"]
["<h></h a='b'>", [["StartTag", "h", {}], "ParseError", ["EndTag", "h"]], "End Tag w/attribute"]
["<h a='b' c='d'>", [["StartTag", "h", {'a':'b', 'c':'d'}]], "Multiple atts"]
["<h a='b'c='d'>", [["StartTag", "h", {'a':'b', 'c':'d'}]], "Multiple atts no space"]
["<h a='b' a='d'>", ["ParseError", ["StartTag", "h", {'a':'b'}]], "Repeated attr"]
["<!--comment-->", [["Comment", "comment",], "Simple comment"]